Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

! xterm 
xterm*faceName: Monospace
xterm*faceSize: 11
xterm*selectToClipboard: true

! urxvt
URxvt*.scrollBar: false
URxvt.font: xft:JetBrainsMono Nerd Font Mono:size=11, \
	        xft:UbuntuMono Nerd Font:size=12, \
            xft:Inconsolata Nerd Font Mono:size=12, \
            xft:DejaVuSansMono Nerd Font:size=12, \
            xft:DroidSansMono Nerd Font:size=12, \
            xft:FuraMono Nerd Font:size=12, \
            xft:RobotoMono Nerd Font:size=12, \
            xft:Noto Sans Mono:size=12, \
            xft:Monospace:style=Medium:size=12   

URxvt.termName: rxvt-256color

URxvt*.keysym.Shift-Control-V: eval:paste_clipboard
URxvt*.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

URxvt*.iso14755: false
URxvt*.iso14755_52: false
URxvt*imLocale: en_US.UTF-8
URxvt.saveLines: 2000
URxvt*.letterspace: -1

URxvt.internalBorder: 0 
vim.internalBorder: 0

URxvt.perl-ext-common: default,resize-font,matcher

URxvt.url-launcher: /usr/bin/xdg-open 
URxvt.matcher.button: 1
! rofi
rofi.font: Hack 11

